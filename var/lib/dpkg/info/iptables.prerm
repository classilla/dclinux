#!/bin/sh -e

if [ "$1" = remove -o "$1" = upgrade ]; then
   if command -v install-docs >/dev/null 2>&1; then
      install-docs -r nat
      install-docs -r packet-filter
   fi
fi
# symlink management generated by debstd
if [ \( "$1" = "upgrade" -o "$1" = "remove" \) -a -L /usr/doc/iptables ]; then
  rm -f /usr/doc/iptables
fi
