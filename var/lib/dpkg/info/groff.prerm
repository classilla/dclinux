#!/bin/sh -e
  # prerm - generated by debian/rules
  #
  # symlink for doc dir generated by debian/rules 
if	[ \( "$1" = "upgrade" -o "$1" = "remove" \) -a -L /usr/doc/groff ] 
then	rm -f /usr/doc/groff 
fi 
  #
  # call to install-info generated by debian/policy 
install-info --quiet --remove /usr/share/info/groff.gz 
  #

